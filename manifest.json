{
  "manifest_version": 3,
  "name": "Saved Content Extension",
  "version": "1.0",
  "description": "A Chrome extension to display saved content.",
  "permissions": ["storage"],
  "background": {
    "service_worker": "background.js"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  }
}

  